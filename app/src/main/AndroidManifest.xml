<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.luis.basedatos">

    <application
        android:allowBackup="true"
        android:icon="@drawable/iconoreservpark"
        android:label="@string/app_name"
        android:roundIcon="@drawable/iconoreservpark"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".views.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".views.RegisterActivity" />
        <activity android:name=".views.LoginActivity"></activity>
    </application>

</manifest>